<h1>Organization Information</h1>
<p><%=@organization.name%></p>
<p><%=@organization.description%></p>
<a href="http://<%=@organization.site_url%>">Visit the <%=@organization.name%> HomePage.</a>
<% @organization.dogs.each do |dog| %>
  <div class='col-md-offset-2 col-md-6 target'>
    <hr>
    <h3> <%= dog.name %> from <%= dog.organization.name %></h3>
    <img src="<%=dog.picture_url%>" alt="<%=dog.name%>" height="250" width="250">
    <%= link_to dog.name, organization_dog_path(dog.organization, dog), class: 'btn btn-default btn-main' %>
  </div>
<% end %>
