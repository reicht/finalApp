<div class='row page-panel'>
  <div class='span1'>
    <div class='span10'>
      <h1>The <%=@group.name%> Group</h1>
      <p><%=@group.description%></p>
      <hr>
      <%@group.breeds.each do |breed|%>
      <div class='col-md-offset-2 col-md-6 target'>
        <hr>
        <h3> The <%= breed.name %>.</h3>
        <%= link_to breed.name, breed, class: 'btn btn-default btn-main' %>
      </div>
      <% end %>
    </div>
  </div>
</div>
