<div class='session-space'>
</div>
<div class='row page-panel session-panel'>
  <div id="clearance" class="sign-up sign-in-wrapper">
    <h2><%= t(".title") %></h2>

    <%= form_for @user do |form| %>
      <%= render partial: '/users/form', object: form %>

      <div class="submit-field">
        <%= form.submit %>
      </div>

      <div class="other-links pull-right">
        <%= link_to t(".sign_in"), sign_in_path %>
      </div>
    <% end %>
  </div>
</div>
